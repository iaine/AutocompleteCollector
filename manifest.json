{

  "description": "Collect results while using search engine autocomplete for later analysis",
  "manifest_version": 2,
  "name": "AutocompleteCollector",
  "version": "0.0.1",
  "homepage_url": "https://github.com/iaine/",

  "permissions": [
    "webRequest", "webRequestBlocking", "webNavigation", "<all_urls>", "storage", "downloads", "activeTab", "tabs",
    "unlimitedStorage"
  ],

  "incognito": "not_allowed", 

"browser_action": {
    "default_popup": "popup/results.html"
  },

  "background": {
    "scripts": [
      "js/db.js", "js/background.js"
    ]
  }
}
